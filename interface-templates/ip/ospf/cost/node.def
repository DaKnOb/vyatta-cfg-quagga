type: u32
help: Set interface cost
syntax:expression: $VAR(@) >= 1 && $VAR(@) <= 65535; "Must be between 1-65535"
update:vyatta-vtysh -c "configure terminal" \
	-c "interface $IFNAME" \
	-c "ip ospf cost $VAR(@)"
delete:vyatta-vtysh -c "configure terminal" \
	-c "interface $IFNAME" \
	-c "no ip ospf cost"
comp_help: possible completions:
 <1-65535>          Set Cost
