tag:

type: u32

help: Rule number (1-998)

syntax:expression: $VAR(@) > 0 && $VAR(@) <= 998; "pbr rule number must be between 1 and 998"

val_help: u32:1-998; Rule number


end: if [ ${COMMIT_ACTION} = 'DELETE' ] ;
     then
       /opt/vyatta/sbin/vyatta-dp-pbr.pl --cmd=delete --group="$VAR(../@)" --rule="$VAR(@)";
     else
       /opt/vyatta/sbin/vyatta-dp-pbr.pl --cmd=update --group="$VAR(../@)" --rule="$VAR(@)";
     fi


