type: ipv4; "router-id must be a IPv4 address"
help: "Override the default router identifier"
update: "/usr/bin/vtysh -c \"configure terminal\" -c \"router ospf\" \
           -c \"ospf router-id $(@)\" "
delete: "/usr/bin/vtysh -c \"configure terminal\" -c \"router ospf\" \
           -c \"no ospf router-id \" "
