tag:
type: ipv4
help: Neighbor IP address
create:expression: "${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
        -c \"neighbor $VAR(@)\"; "
delete:expression: "${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
        -c \"no neighbor $VAR(@)\"; "
