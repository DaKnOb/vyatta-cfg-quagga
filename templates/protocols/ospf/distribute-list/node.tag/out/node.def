multi:
type: txt
help: "Filter outgoing routing updates"
syntax: $(@) in "bgp", "connected", "isis", "kernel", "rip", "static"; "Must be (bgp, connected, isis, kernel, rip, or static"
create: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
            -c \"distribute-list $(../@) out $(@) \"; "
delete: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
            -c \"no distribute-list $(../@) out $(@) \"; "
