help: "Stub OSPF area"
syntax: ! $(../../@) in "0", "0.0.0.0"; "Backbone can't be stub"
syntax: $(../nssa/) == "" ; "Must delete nssa area type first"
syntax: $(../normal/) == "" ; "Must delete normal area type first"
update: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
         -c \"area $(../../@) stub\"; "
delete: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
         -c \"no area $(../../@) stub\"; "
