type: txt
help: "Set OSPF ABR type"
default: "cisco"
syntax: $(@) in "cisco", "ibm", "shortcut", "standard"; "Must be (cisco, ibm, shortcut, standard)"
update: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
        -c \"ospf abr-type $(@)\"; "
delete: "\\${vyatta_sbindir}/vyatta-vtysh.pl -c \"configure terminal\" -c \"router ospf\" \
        -c \"no ospf abr-type $(@)\"; "
