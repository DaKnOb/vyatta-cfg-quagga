help: Silently discard pkts when matched

end: if [ ${COMMIT_ACTION} = 'DELETE' ]; then 
        vtysh -c "configure terminal" \
          -c "no ipv6 route $VAR(../@) null0";
     else
        if [ -n "$VAR(./distance/@)" ]; then
	   DIST="$VAR(./distance/@)";
        fi;
        vtysh -c "configure terminal" \
          -c "ipv6 route $VAR(../@) null0 $DIST";
     fi;
