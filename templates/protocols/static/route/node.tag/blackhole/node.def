help: Set to silently discard pkts when matched

end: if [ ${COMMIT_ACTION} = 'DELETE' ]; then
       vtysh -c "configure terminal" \
                    -c "no ip route $VAR(../@) null0";
     else
       if [ -n "$VAR(./distance/@)" ]; then
	 DIST="$VAR(./distance/@)";
       fi;
       vtysh -c "configure terminal" \
                    -c "ip route $VAR(../@) null0 $DIST";
     fi;
