type: ipv4
help: Set the route-reflector cluster-id
comp_help: \1  <x.x.x.x>\tcluster-id IP
update: vyatta-vtysh -c "configure terminal" -c "router bgp $VAR(../../@)" \
           -c "bgp cluster-id $VAR(@)"
delete: vyatta-vtysh -c "configure terminal" -c "router bgp $VAR(../../@)" \
           -c "no bgp cluster-id"
