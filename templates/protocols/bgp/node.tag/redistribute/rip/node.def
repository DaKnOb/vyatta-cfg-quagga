help: Set to redistribute RIP routes into BGP
delete: touch /tmp/bgp-redist-rip.$PPID
end:  vtysh -c "configure terminal" -c "router bgp $VAR(../../@)" \
        -c "no redistribute rip "; 
      if [ -f "/tmp/bgp-redist-rip.$PPID" ]; then 
        rm -rf /tmp/bgp-redist-rip.$PPID; 
      else 
        if [ -n "$VAR(./metric/@)" ]; then 
          cond="metric $VAR(./metric/@)";
        fi; 
        if [ -n "$VAR(./route-map/@)" ]; then 
          cond="$cond route-map $VAR(./route-map/@)"; 
        fi; 
        vtysh -c "configure terminal" -c "router bgp $VAR(../../@)" \
          -c "redistribute rip $cond"; 
      fi;
