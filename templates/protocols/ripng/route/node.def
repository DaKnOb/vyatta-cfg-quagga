multi:
type: ipv6net
help: Set RIPng static route

syntax:expression: exec "${vyatta_sbindir}/check_prefix_boundary $VAR(@)"

create: vyatta-vtysh -c "configure terminal" \
          -c "router ripng"                                       \
          -c "route $VAR(@)"

delete: vyatta-vtysh -c "configure terminal" \
          -c "router ripng"                                       \
          -c "no route $VAR(@)"

