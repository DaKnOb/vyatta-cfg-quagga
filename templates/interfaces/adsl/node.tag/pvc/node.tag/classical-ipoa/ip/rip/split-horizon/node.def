help: Enable split horizon on this interface
update:expression: "vtysh \
       -c \"configure terminal \"                     \
       -c \"interface $VAR(../../../../../@) \"             \
       -c \"ip rip split-horizon \" "

delete:expression: "vtysh \
       -c \"configure terminal \"                     \
       -c \"interface $VAR(../../../../../@) \"             \
       -c \"no ip rip split-horizon \" "
