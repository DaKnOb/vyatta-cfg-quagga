help: Set MD5 key id
update:expression: "vtysh \
       -c \"configure terminal\"                      \
       -c \"interface $VAR(../../../../../../@) \"          \
       -c \"no ip ospf authentication \"              \
       -c \"ip ospf authentication message-digest\"; "

delete:expression: "vtysh \
       -c \"configure terminal\"                      \
       -c \"interface $VAR(../../../../../../@) \"          \
       -c \"no ip ospf authentication \"; "
