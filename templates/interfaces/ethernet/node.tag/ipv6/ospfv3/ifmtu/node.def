type: u32
help: Set interface MTU
syntax:expression: $VAR(@) >= 1 && $VAR(@) <= 65535; "Must be between 1-65535"
update: vyatta-vtysh                      \
          -c "configure terminal"         \
          -c "interface $VAR(../../../@)" \
          -c "ipv6 ospf6 ifmtu $VAR(@)"
delete: vyatta-vtysh                      \
          -c "configure terminal"         \
          -c "interface $VAR(../../../@)" \
          -c "no ipv6 ospf6 ifmtu"
comp_help: possible completions:
  <1-65535>         Set interface MTU
