help: Disable Maximum Transmission Unit (MTU) mismatch detection
create:expression: "vtysh         \
       -c \"configure terminal\"                              \
       -c \"interface $VAR(../../../../../@).$VAR(../../../@) \" \
       -c \"ip ospf mtu-ignore\"; "

delete:expression: "vtysh         \
       -c \"configure terminal\"                              \
       -c \"interface $VAR(../../../../../@).$VAR(../../../@) \" \
       -c \"no ip ospf mtu-ignore\"; "
